{
  "name": "sy-post-publisher",
  "private": true,
  "version": "0.8.1",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "dev": "BUILD_TYPE=siyuan vite build --watch",
    "build": "BUILD_TYPE=siyuan vite build",
    "stayicBuild": "BUILD_TYPE=static vite build",
    "start": "vite preview",
    "test": "vitest --watch",
    "preview": "vite preview",
    "syncVersion": "python scripts/version.py",
    "parseChangelog": "python scripts/parse_changelog.py",
    "prepareRelease": "pnpm syncVersion && pnpm parseChangelog",
    "widget": "python scripts/widget.py",
    "ext": "python scripts/ext.py",
    "package": "pnpm widget && pnpm ext"
  },
  "devDependencies": {
    "@terwer/eslint-config-custom": "^1.2.0",
    "@types/node": "18",
    "@vitejs/plugin-vue": "^4.1.0",
    "fast-glob": "^3.2.12",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vercel": "^29.4.0",
    "vite": "^4.3.2",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^1.4.2"
  },
  "dependencies": {
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "element-plus": "^2.3.5",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "4"
  }
}
