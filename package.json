{
  "name": "sy-post-publisher",
  "private": true,
  "version": "0.8.1",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "dev": "BUILD_TYPE=siyuan vite build --watch",
    "build": "BUILD_TYPE=siyuan vite build",
    "staticBuild": "BUILD_TYPE=static vite build",
    "start": "vite preview",
    "test": "vitest --watch",
    "preview": "vite preview",
    "syncVersion": "python scripts/version.py",
    "parseChangelog": "python scripts/parse_changelog.py",
    "prepareRelease": "pnpm syncVersion && pnpm parseChangelog",
    "widget": "python scripts/widget.py",
    "ext": "python scripts/ext.py",
    "package": "pnpm widget && pnpm ext && pnpm ext -t firefox"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@terwer/eslint-config-custom": "^1.2.0",
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^4.2.3",
    "fast-glob": "^3.2.12",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "stylus": "^0.59.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.16.2",
    "unplugin-vue-components": "^0.24.1",
    "vercel": "^29.4.0",
    "vite": "^4.3.9",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^1.6.5"
  },
  "dependencies": {
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "element-plus": "^2.3.5",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.1",
    "zhi-blog-api": "^1.3.3",
    "zhi-common": "^1.6.2",
    "zhi-device": "^0.7.3",
    "zhi-env": "^1.17.2",
    "zhi-log": "^1.13.3",
    "zhi-siyuan-api": "^1.3.2"
  }
}
