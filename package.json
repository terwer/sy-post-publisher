    "stylus": "^0.59.0",
    "typescript": "^5.1.3",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.24.1",
    "vercel": "^29.4.0",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1",
